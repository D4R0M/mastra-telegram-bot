{
  "name": "agent-stack-mastra",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "vitest run",
    "dev": "mastra dev",
    "webapp:build": "npm --prefix webapp install --no-package-lock --no-save && npm --prefix webapp run build",
    "build": "npm run webapp:build && mastra build",
    "start": "node --import=./.mastra/output/instrumentation.mjs .mastra/output/index.mjs",
    "check": "tsc",
    "check:format": "prettier --check '**/*.ts'",
    "format": "prettier --write '**/*.ts'",
    "curl:webhook": "bash scripts/curl-webhook.sh",
    "lint": "eslint .",
    "dep:cycles": "madge --circular --extensions ts,tsx src",
    "migrate": "node -r dotenv/config ./node_modules/node-pg-migrate/bin/node-pg-migrate up -m src/db/migrations",
    "migrate:down": "node -r dotenv/config ./node_modules/node-pg-migrate/bin/node-pg-migrate down -m src/db/migrations -c 1"
  },
  "keywords": [],
  "type": "module",
  "author": "",
  "license": "ISC",
  "description": "",
  "engines": {
    "node": ">=20.9.0"
  },
  "dependencies": {
    "@ai-sdk/openai": "^1.3.22",
    "@inngest/realtime": "^0.3.1",
    "@mastra/core": "^0.15.3",
    "@mastra/inngest": "^0.12.3",
    "@mastra/libsql": "^0.13.8",
    "@mastra/loggers": "^0.10.10",
    "@mastra/mcp": "^0.11.4",
    "@mastra/memory": "^0.14.3",
    "@mastra/pg": "^0.14.6",
    "@openrouter/ai-sdk-provider": "^0.4.6",
    "@slack/web-api": "^7.9.3",
    "@types/node-cache": "^4.1.3",
    "@types/pg": "^8.15.5",
    "ai": "^4.3.16",
    "dotenv": "^17.2.0",
    "exa-js": "^1.8.17",
    "inngest": "^3.40.2",
    "inngest-cli": "^1.11.5",
    "node-cache": "^5.1.2",
    "pg": "^8.16.3",
    "pino": "^9.9.4",
    "tsx": "^4.20.3",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@types/node": "^24.5.0",
    "@typescript-eslint/parser": "^7.18.0",
    "ajv": "^6.12.6",
    "eslint": "^8.57.0",
    "eslint-plugin-import": "^2.29.1",
    "madge": "^6.1.0",
    "mastra": "^0.11.3",
    "node-pg-migrate": "^8.0.3",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  }
}
